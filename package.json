{
  "name": "javascript-development-environment",
  "version": "1.0.0",
  "description": "Javascript Development Environment",
  "main": "testingnode.js",
  "scripts": {
    "prestart": "babel-node buildScripts/startMessage.js",
    "start": "npm-run-all --parallel security-check share open:src lint:watch test:watch start-mockapi",
    "open:src": "babel-node buildScripts/srcServer.js",
    "test": "mocha --reporter progress buildScripts/testSetup.js \"src/**/*.test.js\"",
    "test:watch": "npm run test -- --watch",
    "lint": "esw webpack.config.* src buildScripts",
    "lint:watch": "npm run lint -- --watch",
    "security-check": "nsp check",
    "share": "lt --port 3000",
    "generate-mock-data": "babel-node buildScripts/generateMochData",
    "prestart-mockapi": "npm run generate-mock-data",
    "start-mockapi": "json-server --watch src/api/db.json --port 3002"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/asagarw/js-dev-env.git"
  },
  "author": "Ashish Agarwal",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-preset-latest": "^6.24.1",
    "chai": "^3.5.0",
    "chalk": "^1.1.3",
    "css-loader": "^0.28.1",
    "eslint": "^3.19.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-watch": "^3.1.0",
    "express": "^4.15.2",
    "jsdom": "^10.1.0",
    "json-schema-faker": "^0.5.0-rc1",
    "json-server": "^0.10.0",
    "localtunnel": "^1.8.2",
    "mocha": "^3.3.0",
    "npm-run-all": "^4.0.2",
    "numeral": "^2.0.6",
    "open": "0.0.5",
    "style-loader": "^0.17.0",
    "webpack": "^2.5.0",
    "webpack-dev-middleware": "^1.10.2",
    "whatwg-fetch": "^2.0.3"
  },
  "bugs": {
    "url": "https://github.com/asagarw/js-dev-env/issues"
  },
  "homepage": "https://github.com/asagarw/js-dev-env#readme"
}
